<meeting>

  <!--"duration" format is hr:mm:ss-->
  <!--Use the "editable" attribute to allow the user to manually modify an item's duration-->
  <!--Use the "bell" attribute to automatically sound a bell at the expiration of an item's' duration-->
  <items>
    <item name="Welcome" duration="00:03:00" editable="true" />
    <item name="Keynote" duration="01:00:00" editable="true" bell="true"/>
    <item name="Concluding Comments" duration="00:03:00" editable="true" />
  </items>  
  
</meeting>